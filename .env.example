# Tolgee Translation Configuration
# Copy this file to .env.local and fill in your actual values

# Tolgee API URL - Your Tolgee instance URL
VITE_APP_TOLGEE_API_URL=https://tolgee-translation.oddleapp.com

# Tolgee API Key - Get this from your Tolgee project settings
# Format: tgpak_[your_api_key_here]
VITE_APP_TOLGEE_API_KEY=tgpak_
# For CLI operations (same value as above, but different name for Tolgee CLI)
TOLGEE_API_KEY=tgpak_

# Tolgee Project ID (optional) - Your project ID in Tolgee
# Example: 19
VITE_APP_TOLGEE_PROJECT_ID=19

# Development settings
NODE_ENV=development